var x = 3;

switch(x / 2) {
    case 1:
        print "odd number";
    case 0:
        print "even number";
    default:
        print "I dont know";
}

switch(x / 3) {
    case 1:
        print "remainder is 1";
    case 2:
        print "remainder is 2";
    default:
        print "divisible by 3";
}

print "end";
